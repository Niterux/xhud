"UnlitGeneric"
{
	"$translucent" "1"
	"$baseTexture" "vgui\replay\thumbnails\menu\Xloading"
	"$vertexalpha" "1"
	"$vertexcolor" "1"
	"$ignorez" "1"

	"$angle" "45.0"
	"$translate" "[-0.01 0.0]"
	"$center" "[0.5 0.5]"

	"$color" "{255 50 65}"

	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}